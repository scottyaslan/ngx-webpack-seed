  "name": "ngx-webpack-seed",
  "version": "0.0.0",
  "scripts": {
    "start": "npm run watch",
    "clean-and-copy": "node scripts/clean.js && node scripts/copy-files.js",
    "build:webapp:bundle:development": "npm run webpack",
    "build:webapp:bundle:production": "npm run webpack:production",
    "clean:all": "node scripts/clean.js && node scripts/clean-dev.js",
    "clean:install": "npm run clean-and-copy && cd ./target/frontend-working-directory && npm install && npm run test:parallel && npm run build:webapp:bundle:production",
    "clean:install:skipTests": "npm run clean-and-copy && cd ./target/frontend-working-directory && npm install && npm run build:webapp:bundle:production",
    "dev:install": "node scripts/copy-files.js && cd ./target/frontend-working-directory && npm install && npm test && npm  run build:webapp:bundle:development",
    "dev:install:skipTests": "node scripts/copy-files.js && cd ./target/frontend-working-directory && npm install && npm run build:webapp:bundle:development",
    "test": "karma start karma.conf.js --single-run",
    "test:dev": "karma start karma.conf.js",
    "test:parallel": "karma start karma.conf.js --single-run --parallelOptions.executors=4",
    "eslint": "./node_modules/.bin/eslint --ext .ts,.js ./webapp/",
    "eslint:fix": "./node_modules/.bin/eslint --ext .ts,.js ./webapp/ --fix",
    "stylelint": "./node_modules/.bin/stylelint './webapp/theming/**/*.scss'",
    "lint": "npm run eslint && npm run stylelint",
    "serve": "./node_modules/.bin/webpack-dev-server --config webpack.dev.js",
    "watch": "npm run webpack && npm run serve",
    "webpack": "./node_modules/.bin/webpack --config webpack.dev.js --display-error-details",
    "webpack:production": "./node_modules/.bin/webpack --config webpack.prod.js --display-error-details",
    "webpack:minimal": "./node_modules/.bin/webpack --config webpack.minimal.js",
    "lint-staged": "./node_modules/.bin/lint-staged"
  },
  "private": true,
  "description": "The ngx-seed web application.",
